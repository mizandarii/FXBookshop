<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbLoginFormRoot" alignment="TOP_CENTER" prefHeight="180.0" prefWidth="401.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="users.loginform.LoginformController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label fx:id="lbLoginInfo" />
      <HBox prefHeight="20.0" prefWidth="381.0" spacing="5.0">
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="71.0" text="Логин">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfLogin" prefHeight="25.0" prefWidth="257.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox layoutX="20.0" layoutY="20.0" prefHeight="27.0" prefWidth="381.0" spacing="5.0">
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="71.0" text="Пароль">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="tfPassword" prefHeight="25.0" prefWidth="255.0" />
         </children>
      </HBox>
      <Button fx:id="btGoToLogin" mnemonicParsing="false" onAction="#clickLogin" prefHeight="25.0" prefWidth="186.0" text="Войти" />
      
   </children>
</VBox>
